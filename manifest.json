{
    "name": "Github Confirm It",
    "version": "1.0",
    "description": "Quickly add repo name to confirm GitHub repo changes",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
          "matches": ["https://github.com/*/*/settings"],
          "js": ["githubExtends.js"],
          "css": ["sa-styles.css"]
        }
      ],
    "web_accessible_resources": ["images/*.svg"],
    "manifest_version": 2
}
